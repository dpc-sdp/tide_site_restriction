{
  "description": "User restriction by Sites for Tide Drupal 8 distribution",
  "extra": {
    "installer-paths": {
      "docroot/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ]
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ]
  },
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "name": "dpc-sdp/tide_site_restriction",
  "prefer-stable": true,
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "dpc-sdp/tide_core": "dev-reference",
    "dpc-sdp/tide_site": "dev-reference",
    "drupal/form_options_attributes": "^1.0",
    "drupal/select2": "^1.7",
    "oomphinc/composer-installers-extender": "^2.0",
    "select2/select2": "^4.0"
  },
  "suggest": {
    "dpc-sdp/tide_api:^3.0.0": "Allows to use Drupal in headless mode",
    "dpc-sdp/tide_publication:^3.0.0": "Allows to use publication content type"
  },
  "type": "drupal-module"
}
