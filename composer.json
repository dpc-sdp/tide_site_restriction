{
    "name": "dpc-sdp/tide_site_restriction",
    "description": "User restriction by Sites for Tide Drupal 10 distribution",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "require": {
        "dpc-sdp/tide_core": "dev-feature/SRM-870-d10-update",
        "dpc-sdp/tide_site": "dev-feature/SRM-942-drupal-10",
        "drupal/select2": "^1.7",
        "oomphinc/composer-installers-extender": "^2.0",
        "select2/select2": "^4.0",
        "drupal/form_options_attributes": "^2.0"
    },
    "suggest": {
        "dpc-sdp/tide_api:dev-feature/SRM-948-d10-update": "Allows to use Drupal in headless mode",
        "dpc-sdp/tide_publication:dev-feature/SRM-976-drupal-10": "Allows to use publication content type"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ]
        },
        "installer-types": ["bower-asset", "npm-asset"]
    }
}
