{
    "name": "dpc-sdp/tide_site_restriction",
    "description": "User restriction by Sites for Tide Drupal 8 distribution",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "require": {
        "dpc-sdp/tide_core": "^2.0.0",
        "dpc-sdp/tide_site": "^2.0.1",
        "dpc-sdp/tide_publication": "dev-feature/SDPA-5093-remove-old-card-options as ^1.0.0",
        "drupal/select2": "1.7",
        "oomphinc/composer-installers-extender": "^1.1",
        "select2/select2": "^4.0",
        "drupal/form_options_attributes": "^1.0"
    },
    "suggest": {
        "dpc-sdp/tide_api": "^1.5.0"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ]
        },
        "installer-types": ["bower-asset", "npm-asset"]
    }
}
